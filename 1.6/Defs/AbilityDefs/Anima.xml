<VEF.Abilities.AbilityDef ParentName="VPE_PsycastBase">
    <defName>VPE_AnimaReapSTNL</defName>
    <label>Anima Reap</label>
    <description>Rip the energy out of the target, using it to strengthen your connection to the Greater Anima tree.</description>
    <iconPath>UI_Anima/Abilities/Reap</iconPath>
    <castTime>120</castTime>
    <targetMode>Pawn</targetMode>
    <range>14</range>
	<needsTicking>true</needsTicking>
    <goodwillImpact>-100</goodwillImpact>
<abilityClass>VEF.Abilities.Ability_Blank</abilityClass>
    <castSound>DeathAcidifier</castSound>
	<targetingParameters>
			<canTargetLocations>False</canTargetLocations>
			<canTargetSelf>False</canTargetSelf>
			<canTargetAnimals>True</canTargetAnimals>
			<canTargetPawns>True</canTargetPawns>
			<canTargetHumans>True</canTargetHumans>
			<canTargetMechs>True</canTargetMechs>
		</targetingParameters>
    <modExtensions>
      <li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
        <path>VPE_AnimaSTNL</path>
        <level>6</level>
        <order>1</order>
        <psyfocusCost>0.75</psyfocusCost>
        <entropyGain>75</entropyGain>
		<prerequisites>
<li>VPE_SummonWispSTNL</li>
<li>VPE_AnimaVertigoSTNL</li>
</prerequisites>
      </li>
<li Class="VEF.Abilities.AbilityExtension_ExtraHediffs">
<onCaster>
<li>AnimareaperSTNL</li>
</onCaster>
